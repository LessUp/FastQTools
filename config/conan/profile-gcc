# Conan Profile for GCC 15 + libstdc++
# Usage: conan install . -pr:h config/conan/profile-gcc --build=missing

[settings]
arch=x86_64
build_type=Release
compiler=gcc
compiler.cppstd=20
compiler.libcxx=libstdc++11
compiler.version=15
os=Linux

[conf]
tools.build:compiler_executables={"c": "gcc", "cpp": "g++"}
